[user]
  name = Julio Arias
[alias]
  co = checkout
  cm = commit
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
  editor = mvim -f
[apply]
  whitespace = nowarn
[heroku]
  account = personal
[difftool "Kaleidoscope"]
  cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
  prompt = false
[diff]
  tool = Kaleidoscope
